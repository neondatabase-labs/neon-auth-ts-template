{
  "name": "neon-auth-js-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier -c . --log-level warn",
    "format:fix": "prettier -w . --log-level warn",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint": "npm-run-all format lint:ts lint:eslint",
    "lint:fix": "npm-run-all format:fix lint:ts lint:eslint:fix",
    "prepreview": "npm run build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@stackframe/js": "^2.8.12",
    "@tailwindcss/vite": "^4.1.7",
    "tailwindcss": "^4.1.7"
  }
}
